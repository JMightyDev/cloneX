{
	"rewrites": [
		{
			"source": "/x/:path*",
			"destination": "/x/index.html"
		},
		{
			"source": "/x/assets/:path*",
			"destination": "/assets/:path*"
		}
	],
	"headers": [
		{
			"source": "/x/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-cache, no-store, must-revalidate"
				}
			]
		}
	],
	"cleanUrls": true,
	"trailingSlash": false
}
