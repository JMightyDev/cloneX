{
	"rewrites": [
		{
			"source": "/assets/:path*",
			"destination": "/assets/:path*"
		},
		{
			"source": "/:path*",
			"destination": "/:path*"
		}
	],
	"headers": [
		{
			"source": "/assets/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		},
		{
			"source": "/:path*",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-cache, no-store, must-revalidate"
				}
			]
		}
	],
	"cleanUrls": true,
	"trailingSlash": false
}
