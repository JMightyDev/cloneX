{
	"rewrites": [
		{
			"source": "/x/:path*",
			"destination": "/index.html"
		},
		{
			"source": "/x",
			"destination": "/index.html"
		}
	],
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "/x/assets/(.*)",
			"dest": "/assets/$1"
		},
		{
			"src": "/x/(.*)",
			"dest": "/index.html"
		}
	],
	"headers": [
		{
			"source": "/x/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		}
	],
	"cleanUrls": true,
	"trailingSlash": false
}
