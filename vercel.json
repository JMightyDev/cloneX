{
	"rewrites": [
		{
			"source": "/x/:path*",
			"destination": "/index.html"
		},
		{
			"source": "/x",
			"destination": "/index.html"
		},
		{
			"source": "/x/assets/:path*",
			"destination": "/assets/:path*"
		}
	],
	"headers": [
		{
			"source": "/x/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=0, must-revalidate"
				}
			]
		}
	],
	"cleanUrls": true,
	"trailingSlash": false
}
